<launch>
	<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
        <rosparam file="$(find p3at_slam)/navigation/common/costmap_common_params_p3at.yaml" command="load" ns="global_costmap" />
        <rosparam file="$(find p3at_slam)/navigation/common/costmap_common_params_p3at.yaml" command="load" ns="local_costmap" />
        <rosparam file="$(find p3at_slam)/navigation/common/local_costmap_params.yaml" command="load" />
        <rosparam file="$(find p3at_slam)/navigation/local_navigation/global_costmap_params.yaml" command="load" />
        <rosparam file="$(find p3at_slam)/navigation/common/base_local_planner_params.yaml" command="load"/>
        <rosparam file="$(find p3at_slam)/navigation/common/global_planner_params.yaml" command="load" />
        <rosparam file="$(find p3at_slam)/navigation/common/recovery_behaviors.yaml" command="load" />
        <rosparam>
            planner_frequency: 1.0 
        </rosparam>
        <param name="base_global_planner" value="global_planner/GlobalPlanner"/>
    </node>
</launch>
